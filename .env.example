# Job Board Aggregator Environment Configuration
# Copy this file to .env and fill in your actual values

# API Authentication
API_AUTH_HASH=your_api_auth_hash_here

# Pinecone Vector Database Configuration
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=us-east-1-aws
PINECONE_INDEX_NAME=job-board-index
PINECONE_NAMESPACE=default
PINECONE_EMBEDDING_MODEL=llama-text-embed-v2

# Groq API Configuration
GROQ_API_KEY=your_groq_api_key_here
GROQ_MODEL=llama3-70b-8192
GROQ_MAX_RETRIES=3
GROQ_REQUESTS_PER_MINUTE=24
GROQ_MODEL_ROTATION_ENABLED=true
GROQ_MODELS=llama3-70b-8192,llama-3.3-70b-versatile,deepseek-r1-distill-llama-70b,meta-llama/llama-4-scout-17b-16e-instruct,moonshotai/kimi-k2-instruct,llama-3.1-8b-instant,openai/gpt-oss-120b,openai/gpt-oss-20b,qwen/qwen3-32b
GROQ_LFU_WINDOW_MINUTES=1

# Supabase Database Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here
USE_SUPABASE_DATABASE=true

# Cerebras API Configuration
CERABRAS_API_KEY=your_cerebras_api_key_here

# Cerebras AI False Positive Validation Configuration
CEREBRAS_VALIDATION_ENABLED=true
CEREBRAS_RANDOM_MODEL_COUNT=2
CEREBRAS_MAX_JOBS_PER_BATCH=25
CEREBRAS_RESUME_MAX_CHARS=15000
CEREBRAS_REQUIRE_UNANIMOUS=true
CEREBRAS_SCHEMA_ENFORCEMENT=true
CEREBRAS_TIMEOUT_SECONDS=45

# Logging Configuration
# Set to true to enable minimal logging (only warnings/errors, no verbose output)
# Set to false for detailed logging with timestamps and full information
MINIMAL_LOGGING=false